    "migrate:users": "tsx src/scripts/migrate-users.ts"
  },
  "devDependencies": {
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "mongoose": "catalog:",
    "dotenv": "catalog:",
    "zod": "catalog:"
  }
}
